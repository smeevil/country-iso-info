  LocalesFor: (country)->
    @_data[country]?.locales

  CurrencyFor: (country)->
    @_data[country]?.currency

  NameFor: (country)->
    @_data[country]?.name

  RegionFor: (country)->
    @_data[country]?.region

  SubregionFor: (country)->
    @_data[country]?.subregion

  Regions: ->
    _.compact(_.unique(_.map @_data, (d)-> d.region))

  SubRegionsIn: (region) ->
    _.compact(_.unique(_.map @_data, (d)-> d.subregion if d.region==region))

  CountriesInRegion: (region)->
    _.compact(_.unique(_.map @_data, (d)-> d.value if d.region==region))

  CountriesInSubregion: (subregion)->
    _.compact(_.unique(_.map @_data, (d)-> d.value if d.subregion==subregion))

  NameForLocale: (locale)->
    for item in _.compact(_.unique(_.flatten(_.map(@_data, (d)-> d.locales))))
      return item.name if item.locale.toLowerCase()==locale.toLowerCase()

  HtmlOptionsForCountrySelect: (selected=null)->
    _.map( _.sortBy(@_data, (d)-> d.name), (d)=>
      if @options.countriesForSelectWhiteList.length
        return unless _.contains(@options.countriesForSelectWhiteList, d.value)

      if @options.countriesForSelectBlackList.length
        return if _.contains(@options.countriesForSelectBlackList, d.value)

      @_ToHtmlOptions(d.name, d.value, selected)
    ).join('')

  HtmlOptionsForLanguageSelect: (country, selected=null)->
    supportedFullLocales = @options.supportedLocales
    supportedLanguages = _.map(@options.supportedLocales, (l)-> l.split('-')[0])
    languages=[]
    elements=_.map(@_data[country]?.locales, (d)=>
      #limit to only the supported locales
      if supportedFullLocales.length
        #if we have a direct hit, just use that
        if _.contains(supportedFullLocales, d.locale)
          languages.push(d.locale.split('-')[0]);
          return @_ToHtmlOptions(d.name, d.locale, selected)

        #Maybe we have the language but not the dialect, if so, use that
        language=d.locale.split('-')[0]
        if _.contains(supportedLanguages, language)
          locale = "#{language}-#{language.toUpperCase()}"
          languages.push language
          selected = locale if language == selected.split('-')[0] #update selected element to this if the language matches
          return @_ToHtmlOptions(@NameForLocale(locale), locale, selected)

      #all locales are supported
      else
        languages.push(d.locale.split('-')[0]);
        return @_ToHtmlOptions(d.name, d.locale, selected)
    )
    #are any default languages supplied?
    if @options.languageSelectAlwaysIncludes.length
      #Make sure all the default languages are added
      for set in @options.languageSelectAlwaysIncludes
        elements.push @_ToHtmlOptions(@NameForLocale(set), set) unless _.contains(languages, set.split('-')[0])

    elements.join('')


  _ToHtmlOptions: (name, value, selected)->
    selected=if (selected? and value==selected) then ' SELECTED' else ''
    "<option value='#{value}'#{selected}>#{name}</option>"

